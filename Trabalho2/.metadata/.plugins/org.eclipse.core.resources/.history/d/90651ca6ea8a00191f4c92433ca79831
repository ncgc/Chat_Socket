	private InputStream cliente;
	private Server servidor;
	
	public TrataConexao(InputStream cliente, Server servidor) {
		this.cliente = cliente;
		this.servidor = servidor;
	}

	public void run() {
		Scanner s = new Scanner(this.cliente);
		while (s.hasnextline()) {
			
		}
	}
}
